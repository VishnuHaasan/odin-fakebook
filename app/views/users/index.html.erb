<h1>Find Friends: </h1>
<%= link_to "Log out",destroy_user_session_path, method: :delete %>
<% @users.each do |user| %>
  <%= user.name %><br>
  <%= form_with url: "/users", method: :get do |f|%>
    <%= hidden_field_tag :requested_user, user.id %>
    <%= f.submit "Send Request" %><br>
  <% end %>
  ----------------------------------------------------------<br>
<% end %>
<%= link_to "Back to Timeline", root_path %>